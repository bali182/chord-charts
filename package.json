{
  "name": "chord-chart",
  "version": "0.0.1",
  "repository": "https://github.com/bali182/chord-charts.git",
  "author": "Édes Balázs <edesbalazs@gmail.com>",
  "license": "MIT",
  "husky": {
    "__hooks": {
      "pre-commit": "yarn run build && git add docs"
    }
  },
  "scripts": {
    "dev:electron": "cross-env NODE_ENV=development webpack --config webpack.electron.js --mode development && npm run start:electron",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.js --mode development",
    "start:electron": "electron dist/main.js",
    "electron:serve": "wait-on http-get://localhost:4000/ && npm run dev:electron",
    "start": "npm-run-all -p dev:react dev:electron",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest --detectOpenHandles --testTimeout=3000000"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/classnames": "^2.2.9",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fluent-ffmpeg": "^2.1.16",
    "@types/puppeteer": "^5.4.2",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-tooltip": "^4.2.4",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "cross-env": "^7.0.3",
    "electron": "^11.2.0",
    "electron-devtools-installer": "^3.1.1",
    "emotion": "^10.0.17",
    "nanoid": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^5.5.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^7.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.1.2",
    "react-sortable-hoc": "^1.11.0",
    "react-tiny-popover": "^6.0.3",
    "react-tooltip": "^4.2.11",
    "redux": "^4.0.5",
    "styled-components": "^3.2.3",
    "videoshow": "^0.1.12",
    "wait-on": "^5.2.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^24.0.19",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "^9.4.2",
    "css-loader": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.1",
    "jest": "^24.9.0",
    "precss": "^4.0.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-merge": "^4.1.5"
  }
}
